{
  "name": "snow-ctp",
  "version": "0.1.2",
  "description": "nodejs ctp wrapper",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/wh1100717/snow-ctp.git"
  },
  "keywords": [
    "nodejs",
    "node",
    "ctp"
  ],
  "author": "wh1100717",
  "license": "MIT",
  "devDependencies": {
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.6.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "node-gyp": "^3.8.0"
  },
  "scripts": {
    "test": "node test.js",
    "build-script": "node ./scripts/index.js",
    "build-ctp": "node-gyp rebuild",
    "build-copy": "cpx ./build/Release/*.node ./src/ && cpx ./scripts/ctp_data_type.js ./src/ && cpx ./scripts/ctp_struct.js ./src/",
    "build": "npm run build-script && npm run build-ctp && npm run build-copy",
    "debug": "npm run build-ctp && npm run build-copy"
  }
}
